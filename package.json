{
  "name": "@zed-industries/vscode-langservers-extracted",
  "description": "",
  "version": "5.0.0-pre1",
  "author": "hrsh7th",
  "license": "MIT",
  "private": true,
  "repository": "hrsh7th/vscode-langservers-extracted.git",
  "scripts": {
    "check": "npx prettier --check . && npm run depcheck",
    "depcheck": "npx syncpack list-mismatches && for dir in css eslint html json; do npx depcheck packages/$dir; done && scripts/depcheck.js",
    "fix": "npx syncpack fix-mismatches && npx syncpack format && npx prettier --write .",
    "release": "np --no-yarn"
  },
  "dependencies": {
    "@zed-industries/vscode-css-language-server": "file:packages/css",
    "@zed-industries/vscode-eslint-language-server": "file:packages/eslint",
    "@zed-industries/vscode-html-language-server": "file:packages/html",
    "@zed-industries/language-server": "file:packages/json"
  },
  "workspaces": [
    "packages/css",
    "packages/eslint",
    "packages/html",
    "packages/json",
    "packages/all"
  ]
}
